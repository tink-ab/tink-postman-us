			"key": "template",
			"value": "<html>\n<head>\n    <script src=\"https://cdnjs.cloudflare.com/ajax/libs/clipboard.js/2.0.0/clipboard.min.js\"></script>\n    <script src=\"https://cdn.jsdelivr.net/npm/jquery@3.4.1/dist/jquery.min.js\"></script>\n</head>\n<body>\n    <div>\n    <h2>TinkLink</h2>\n    <div>\n        <pre><code style=\"width:max-content;\" id=\"live\">{{tinkLinkLive}}</code></pre>\n    </div>\n    <button class=\"copyButton\" type=\"button\" data-clipboard-action=\"copy\" data-clipboard-target=\"#live\" style=\"background:blue;color:white;\">Copy link live</button>\n    </div> \n    <div>\n        <pre><code style=\"width:max-content;\" id=\"local\">{{tinkLinkLocal}}</code></pre>\n    </div> \n    <button class=\"copyButton\" type=\"button\" data-clipboard-action=\"copy\" data-clipboard-target=\"#local\" style=\"background:blue;color:white;\">Copy link localhost</button>\n    </div>  \n</body>\n</html>\n<script>\n    var clipboard = new ClipboardJS('.copyButton');\n    clipboard.on('success', function(e) {\n        e.clearSelection();\n        e.trigger.textContent = 'Copied!';\n        window.setTimeout(function() {\n            e.trigger.textContent = 'Copy link again';\n        }, 2000);\n    });\n    clipboard.on('error', function(e) {\n        e.clearSelection();\n        e.trigger.textContent = 'Not Copied';\n        window.setTimeout(function() {\n            e.trigger.textContent = 'Copy link again';\n        }, 2000);\n    });\n</script>",
